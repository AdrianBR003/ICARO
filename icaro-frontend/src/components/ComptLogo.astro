---
import LogoEPSL from '@/assets/logoepsl.webp';
import LogoUJA  from '@/assets/logouja.webp';
---
<div style="display: flex; align-items: center; gap: 16px;">
  <img
    src={LogoEPSL.src}
    alt="EPSL Logo"
    width="100"
    height="auto"
    loading="lazy"
  />
  <img
    src={LogoUJA.src}
    alt="UJA Logo"
    width="120"
    height="auto"
    loading="lazy"
  />
</div>
